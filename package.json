{
  "name": "free4talk-streaming",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "12.22.7",
    "yarn": "1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged"
    }
  },
  "lint-staged": {
    "src/*": [
      "pretty-quick --staged"
    ],
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "node dest/server.js",
    "build": "node tools/build.js && pkg ./dest/index.js --options no-warnings --out-path ./build && rm -rf ./dest/index.js ./dest/index.js.map"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "pkg": "^5.4.1",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
